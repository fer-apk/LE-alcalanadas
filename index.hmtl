            background-color: #b8b8b8;
        }

        /* Header */
        .header {
            background-color: #c73232;
            padding: 20px 40px;
            display: flex;
            justify-content: space-between;
            align-items: center;
        }

        .logo-section {
            display: flex;
            align-items: center;
            gap: 15px;
        }

        .logo-circle {
            background-color: white;
            width: 65px;
            height: 65px;
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
        }

        .logo-text {
            color: #c73232;
            font-size: 28px;
            font-weight: bold;
        }

        .brand-text {
            color: white;
        }

        .brand-text .line1 {
            font-size: 24px;
            font-weight: bold;
            letter-spacing: 2px;
        }

        .brand-text .line2 {
            font-size: 20px;
            font-weight: bold;
            letter-spacing: 3px;
        }

        .contact-section {
            display: flex;
            align-items: center;
            gap: 50px;
        }

        .catalog-link {
            color: white;
            text-decoration: none;
            font-size: 18px;
        }

        .catalog-link:hover {
            text-decoration: underline;
        }

        .phone-link {
            display: flex;
            align-items: center;
            gap: 15px;
            color: white;
            text-decoration: none;
        }

        .phone-icon {
            width: 35px;
            height: 35px;
        }

        .phone-number {
            font-size: 32px;
            font-weight: bold;
        }

        /* Main Content */
        .main-content {
            display: flex;
            padding: 60px 40px;
            gap: 50px;
            align-items: center;
        }

        .text-section {
            flex: 1;
            padding-right: 40px;
        }

        .title {
            font-size: 68px;
            font-weight: bold;
            color: #c73232;
            line-height: 1.1;
            margin-bottom: 10px;
        }

        .subtitle {
            font-size: 56px;
            color: #666;
            line-height: 1.2;
            margin-bottom: 30px;
        }

        .subtitle .highlight {
            color: #c73232;
            font-weight: bold;
        }

        .description {
            font-size: 28px;
            color: white;
            font-style: italic;
            margin-bottom: 40px;
        }

        .features {
            display: flex;
            gap: 40px;
            align-items: center;
            margin-bottom: 50px;
            font-size: 24px;
            font-weight: bold;
        }

        .divider {
            width: 2px;
            height: 40px;
            background-color: #333;
        }

        .cta-button {
            display: inline-flex;
            align-items: center;
            gap: 15px;
            background-color: #25d366;
            color: white;
            text-decoration: none;
            padding: 20px 40px;
            border-radius: 50px;
            font-size: 28px;
            font-weight: bold;
            transition: background-color 0.3s;
        }

        .cta-button:hover {
            background-color: #20ba5a;
        }

        .whatsapp-icon {
            width: 45px;
            height: 45px;
            fill: white;
        }

        /* Carousel Section */
        .carousel-section {
            flex: 1;
            position: relative;
        }

        .carousel-container {
            position: relative;
            width: 100%;
            max-width: 600px;
            height: 500px;
            border-radius: 20px;
            overflow: hidden;
            box-shadow: 0 10px 30px rgba(0,0,0,0.3);
        }

        .carousel-slide {
            position: absolute;
            width: 100%;
            height: 100%;
            opacity: 0;
            transition: opacity 0.5s ease-in-out;
        }

        .carousel-slide.active {
            opacity: 1;
        }

        .carousel-slide img {
            width: 100%;
            height: 100%;
            object-fit: cover;
        }

        /* Carousel Dots */
        .carousel-dots {
            position: absolute;
            bottom: 30px;
            left: 50%;
            transform: translateX(-50%);
            display: flex;
            gap: 15px;
            z-index: 10;
        }

        .dot {
            width: 15px;
            height: 15px;
            border-radius: 50%;
            background-color: rgba(255,255,255,0.5);
            cursor: pointer;
            transition: background-color 0.3s;
        }

        .dot.active {
            background-color: white;
        }

        /* Fixed WhatsApp Button */
        .fixed-whatsapp {
            position: fixed;
            bottom: 30px;
            right: 30px;
            z-index: 1000;
            cursor: pointer;
            transition: transform 0.3s;
        }

        .fixed-whatsapp:hover {
            transform: scale(1.1);
        }

        .fixed-whatsapp img {
            width: 70px;
            height: 70px;
            filter: drop-shadow(0 4px 8px rgba(0,0,0,0.3));
        }

        /* Demo content for scroll */
        .demo-content {
            height: 200vh;
            padding: 40px;
            background: linear-gradient(to bottom, #b8b8b8, #888);
        }

        .demo-content h2 {
            color: #333;
            margin-bottom: 20px;
        }

        /* Responsive */
        @media (max-width: 1200px) {
            .main-content {
                flex-direction: column;
            }
            
            .title {
                font-size: 52px;
            }
            
            .subtitle {
                font-size: 42px;
            }
        }
    </style>
</head>
