    <script>
        let currentSlide1 = 0;
        const track1 = document.getElementById('carouselTrack1');
        const cards1 = document.querySelectorAll('.card-wrapper1');
        const totalSlides1 = Math.ceil(cards1.length / 3);
        const dotsContainer1 = document.getElementById('carouselDots1');

        // Crear dots dinámicamente
        for (let i = 0; i < totalSlides1; i++) {
            const dot = document.createElement('div');
            dot.classList.add('dot3');
            if (i === 0) dot.classList.add('active');
            dot.onclick = () => goToSlide1(i);
            dotsContainer1.appendChild(dot);
        }

        function moveCarousel1(direction) {
            currentSlide1 += direction;
            
            if (currentSlide1 < 0) {
                currentSlide1 = totalSlides1 - 1;
            } else if (currentSlide1 >= totalSlides1) {
                currentSlide1 = 0;
            }
            
            updateCarousel1();
        }

        function goToSlide1(index) {
            currentSlide1 = index;
            updateCarousel1();
        }

        function updateCarousel1() {
            const cardWidth = cards1[0].offsetWidth + 30;
            const offset = -currentSlide1 * cardWidth * 3;
            track1.style.transform = `translateX(${offset}px)`;
            
            // Actualizar dots
            document.querySelectorAll('.dot3').forEach((dot, index) => {
                dot.classList.toggle('active', index === currentSlide1);
            });
        }

        // Auto-play opcional
        let autoplayInterval1 = setInterval(() => {
            moveCarousel1(1);
        }, 5000);

        // Pausar autoplay al interactuar
        document.querySelector('.carousel-wrapper1').addEventListener('mouseenter', () => {
            clearInterval(autoplayInterval1);
        });

        document.querySelector('.carousel-wrapper1').addEventListener('mouseleave', () => {
            autoplayInterval1 = setInterval(() => {
                moveCarousel1(1);
            }, 5000);
        });

        // Responsive: actualizar en resize
        window.addEventListener('resize', updateCarousel1);
    </script>
